{
  "version": 2,
  "builds": [
    {
      "src": "services/oracle/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    { "src": "/health", "dest": "services/oracle/dist/index.js" },
    { "src": "/oracle", "dest": "services/oracle/dist/index.js", 
"methods": ["POST"] },
    { "src": "/feedback", "dest": "services/oracle/dist/index.js" }
  ]
}

