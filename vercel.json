{
  "version": 2,
  "name": "mirrornode-oracle",
  "builds": [
    {
      "src": "services/oracle/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "services/oracle/dist/index.js"
    },
    {
      "src": "/oracle",
      "dest": "services/oracle/dist/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/feedback",
      "dest": "services/oracle/dist/index.js"
    }
  ]
}
