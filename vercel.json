{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/health$",
      "dest": "dist/index.js"
    },
    {
      "src": "^/oracle$",
      "methods": ["POST"],
      "dest": "dist/index.js"
    },
    {
      "src": "^/feedback$",
      "methods": ["POST"],
      "dest": "dist/index.js"
    }
  ]
}

